{
  "name": "linkedin-formatter",
  "version": "1.0.0",
  "description": "Web extension that allows to bold and italicize text on the LinkedIn website.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viclafouch/linkedin-formatter.git"
  },
  "author": "Victor de la Fouchardiere",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viclafouch/linkedin-formatter/issues"
  },
  "homepage": "https://www.linkedin-formatter.app",
  "keywords": [
    "extension",
    "chrome",
    "react",
    "linkedin",
    "bold",
    "italic"
  ],
  "scripts": {
    "dev": "npm run dev:chrome",
    "dev:chrome": "npx cross-env TARGET=chrome webpack --mode=development",
    "build": "npm-run-all -n build:*",
    "build:chrome": "npx cross-env TARGET=chrome webpack --mode=production",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "publish": "ts-node -T tasks/publish.ts",
    "lint": "tsc --noEmit && eslint . --ext .js,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@emotion/babel-plugin": "11.3.0",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@types/adm-zip": "0.4.34",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@types/react-dom": "17.0.11",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "@viclafouch/eslint-config-viclafouch": "3.3.3",
    "adm-zip": "0.5.9",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "9.0.1",
    "eslint": "8.3.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-airbnb-typescript": "16.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.3",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-environment-jsdom": "27.3.1",
    "jest-environment-jsdom-global": "3.0.0",
    "jest-extended": "1.1.0",
    "npm-run-all": "4.1.5",
    "open": "8.4.0",
    "prettier": "2.5.0",
    "puppeteer": "11.0.0",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.5.2",
    "webpack": "5.62.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1"
  },
  "dependencies": {
    "@emotion/css": "11.5.0",
    "@emotion/react": "11.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "dotenv": "10.0.0",
    "react": "17.0.2",
    "react-detect-click-outside": "1.1.1",
    "react-dom": "17.0.2"
  }
}
