    "@emotion/babel-preset-css-prop": "11.10.0",
    "@types/adm-zip": "0.5.0",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/jest": "29.2.4",
    "@types/node": "^18.11.17",
    "@types/react-dom": "18.0.10",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@typescript-eslint/eslint-plugin": "5.47.0",
