{
  "name": "linkedin-formatter",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run dev:chrome",
    "dev:chrome": "npx cross-env TARGET=chrome webpack --mode=development",
    "dev:firefox": "npx cross-env TARGET=firefox webpack --mode=development",
    "build": "npm-run-all -n build:*",
    "build:chrome": "npx cross-env TARGET=chrome webpack --mode=production",
    "build:firefox": "npx cross-env TARGET=firefox webpack --mode=production",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "publish": "ts-node -T tasks/publish.ts",
    "lint": "tsc --noEmit && eslint .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@emotion/babel-plugin": "11.3.0",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@types/adm-zip": "0.4.34",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@types/react-dom": "17.0.10",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "adm-zip": "0.5.9",
    "babel-loader": "8.2.3",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "9.0.1",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-jest-formatting": "3.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-environment-jsdom": "27.3.1",
    "jest-environment-jsdom-global": "3.0.0",
    "jest-extended": "1.1.0",
    "npm-run-all": "4.1.5",
    "open": "8.4.0",
    "prettier": "2.4.1",
    "puppeteer": "10.4.0",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.4.4",
    "webpack": "5.61.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1"
  },
  "dependencies": {
    "@emotion/css": "11.5.0",
    "@emotion/react": "11.5.0",
    "@emotion/styled": "11.3.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "dotenv": "10.0.0",
    "react": "17.0.2",
    "react-detect-click-outside": "1.1.1",
    "react-dom": "17.0.2"
  }
